type User {
  id: ID! @id
  name: String!
  email: String!
  displayName: String!
}

type Item {
id: ID! @id
tag: String!
title: String!
description: String!
createAt: DateTime!
updatedAt: DateTime!
}

type Post {
id: ID! @id
tag: String!
title: String!
description: String!
createAt: DateTime!
updatedAt: DateTime!
}
